{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
                "singleQuote": true,
                "parser": "flow"
            }
        ]
    }
}